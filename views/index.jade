doctype html
html(ng-app="myApp")
  head
    meta(charset='utf-8')
    base(href='/')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    meta(name='description', content='')
    meta(name='author', content='')
    title Lansing Motorcycle Club
    // Bootstrap Core CSS
    link(href='stylesheets/bootstrap.min.css', rel='stylesheet', type='text/css')
    // Fonts
    link(href='font-awesome/css/font-awesome.min.css', rel='stylesheet', type='text/css')
    link(href='stylesheets/animate.css', rel='stylesheet')
    // Squad theme CSS
    link(href='stylesheets/style.css', rel='stylesheet')
    link(href='color/default.css', rel='stylesheet')
  body#page-top(data-spy='scroll', data-target='.navbar-custom')
    // Preloader
    #preloader
      #load
    nav.navbar.navbar-custom.navbar-fixed-top(role='navigation')
      .container
        .navbar-header.page-scroll
          button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-main-collapse')
            i.fa.fa-bars
          a.navbar-brand(href='/')
            h1 LANSING MOTORCYCLE CLUB
        // Collect the nav links, forms, and other content for toggling
        .collapse.navbar-collapse.navbar-right.navbar-main-collapse
          ul.nav.navbar-nav
            li.active
              a(href='/') Home
            li
              a(href='/results') Results
            li
              a(href='/events') Events
            li
              a(href='/contact') Contact
        // /.navbar-collapse
      // /.container
    // Section: intro
    block content
    div(ng-view)
    footer
            p
              | ©Copyright 2014 - Squad. All rights reserved. 
              a(href='http://bootstraptaste.com/') Bootstrap Themes
              |  by BootstrapTaste
            //
              All links in the footer should remain intact.
              Licenseing information is available at: http://bootstraptaste.com/license/
              You can buy this theme without footer links online at: http://bootstraptaste.com/buy/?theme=Squadfree
    // Core JavaScript Files
    script(src='javascripts/jquery.min.js')
    script(src='javascripts/bootstrap.min.js')
    script(src='javascripts/jquery.easing.min.js')
    script(src='javascripts/jquery.scrollTo.js')
    script(src='javascripts/wow.min.js')
    // Custom Theme JavaScript
    script(src='javascripts/custom.js')
    script(src='javascripts/angular/angular.js')
    script(src='javascripts/app.js')
    script(src='javascripts/services.js')
    script(src='javascripts/controllers.js')
    script(src='javascripts/filters.js')
    script(src='javascripts/directives.js')