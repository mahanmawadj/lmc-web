extends layout

block content
    section#intro.intro
      .slogan
        h2
          | WELCOME TO 
          span.text_color LMC
        h4 Home of the historic Jack Pine enduro race.
      .page-scroll
        a.btn.btn-circle(href='#service')
          i.fa.fa-angle-double-down.animated
    // /Section: intro
    // Section: about
    section#about.home-section.text-center
      .heading-about
        .container
          .row
            .col-lg-8.col-lg-offset-2
              .wow.bounceInDown(data-wow-delay='0.4s')
                .section-heading
                  h2 About us
                  i.fa.fa-2x.fa-angle-down
      .container
        .row
          .col-lg-2.col-lg-offset-5
            hr.marginbot-50
        .row
          .col-xs-6.col-sm-3.col-md-3
            .wow.bounceInUp(data-wow-delay='0.2s')
              .team.boxed-grey
                .inner
                  h5 Anna Hanaceck
                  p.subtitle Pixel Crafter
                  .avatar
                    img.img-responsive.img-circle(src='img/team/1.jpg', alt='')
          .col-xs-6.col-sm-3.col-md-3
            .wow.bounceInUp(data-wow-delay='0.5s')
              .team.boxed-grey
                .inner
                  h5 Maura Daniels
                  p.subtitle Ruby on Rails
                  .avatar
                    img.img-responsive.img-circle(src='img/team/2.jpg', alt='')
          .col-xs-6.col-sm-3.col-md-3
            .wow.bounceInUp(data-wow-delay='0.8s')
              .team.boxed-grey
                .inner
                  h5 Jack Briane
                  p.subtitle jQuery Ninja
                  .avatar
                    img.img-responsive.img-circle(src='img/team/3.jpg', alt='')
          .col-xs-6.col-sm-3.col-md-3
            .wow.bounceInUp(data-wow-delay='1s')
              .team.boxed-grey
                .inner
                  h5 Tom Petterson
                  p.subtitle Typographer
                  .avatar
                    img.img-responsive.img-circle(src='img/team/4.jpg', alt='')
    // /Section: about
    // Section: services
    section#service.home-section.text-center.bg-gray
      .heading-about
        .container
          .row
            .col-lg-8.col-lg-offset-2
              .wow.bounceInDown(data-wow-delay='0.4s')
                .section-heading
                  h2 Our Services
                  i.fa.fa-2x.fa-angle-down
      .container
        .row
          .col-lg-2.col-lg-offset-5
            hr.marginbot-50
        .row
          .col-sm-3.col-md-3
            .wow.fadeInLeft(data-wow-delay='0.2s')
              .service-box
                .service-icon
                  img(src='img/icons/service-icon-1.png', alt='')
                .service-desc
                  h5 Print
                  p
                    | Vestibulum tincidunt enim in pharetra malesuada. Duis semper magna metus electram accommodare.
          .col-sm-3.col-md-3
            .wow.fadeInUp(data-wow-delay='0.2s')
              .service-box
                .service-icon
                  img(src='img/icons/service-icon-2.png', alt='')
                .service-desc
                  h5 Web Design
                  p
                    | Vestibulum tincidunt enim in pharetra malesuada. Duis semper magna metus electram accommodare.
          .col-sm-3.col-md-3
            .wow.fadeInUp(data-wow-delay='0.2s')
              .service-box
                .service-icon
                  img(src='img/icons/service-icon-3.png', alt='')
                .service-desc
                  h5 Photography
                  p
                    | Vestibulum tincidunt enim in pharetra malesuada. Duis semper magna metus electram accommodare.
          .col-sm-3.col-md-3
            .wow.fadeInRight(data-wow-delay='0.2s')
              .service-box
                .service-icon
                  img(src='img/icons/service-icon-4.png', alt='')
                .service-desc
                  h5 Cloud System
                  p
                    | Vestibulum tincidunt enim in pharetra malesuada. Duis semper magna metus electram accommodare.
    // /Section: services
    // Section: contact
    section#contact.home-section.text-center
      .heading-contact
        .container
          .row
            .col-lg-8.col-lg-offset-2
              .wow.bounceInDown(data-wow-delay='0.4s')
                .section-heading
                  h2 Get in touch
                  i.fa.fa-2x.fa-angle-down
      .container
        .row
          .col-lg-2.col-lg-offset-5
            hr.marginbot-50
        .row
          .col-lg-8
            .boxed-grey
              form#contact-form
                .row
                  .col-md-6
                    .form-group
                      label(for='name')
                        | Name
                      input#name.form-control(type='text', placeholder='Enter name', required='required')
                    .form-group
                      label(for='email')
                        | Email Address
                      .input-group
                        span.input-group-addon
                          span.glyphicon.glyphicon-envelope
                        input#email.form-control(type='email', placeholder='Enter email', required='required')
                    .form-group
                      label(for='subject')
                        | Subject
                      select#subject.form-control(name='subject', required='required')
                        option(value='na', selected='') Choose One:
                        option(value='service') General Customer Service
                        option(value='suggestions') Suggestions
                        option(value='product') Product Support
                  .col-md-6
                    .form-group
                      label(for='name')
                        | Message
                      textarea#message.form-control(name='message', rows='9', cols='25', required='required', placeholder='Message')
                  .col-md-12
                    button#btnContactUs.btn.btn-skin.pull-right(type='submit')
                      | Send Message
          .col-lg-4
            .widget-contact
              h5 Main Office
              address
                strong Squas Design, Inc.
                br
                | 				  Tower 795 Folsom Ave, Beautiful Suite 600
                br
                | 				  San Francisco, CA 94107
                br
                abbr(title='Phone') P:
                |  (123) 456-7890
              address
                strong Email
                br
                a(href='mailto:#') email.name@example.com
              address
                strong We're on social networks
                br
                ul.company-social
                  li.social-facebook
                    a(href='#', target='_blank')
                      i.fa.fa-facebook
                  li.social-twitter
                    a(href='#', target='_blank')
                      i.fa.fa-twitter
                  li.social-dribble
                    a(href='#', target='_blank')
                      i.fa.fa-dribbble
                  li.social-google
                    a(href='#', target='_blank')
                      i.fa.fa-google-plus
    // /Section: contact

